<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Trees.MainPage">
    <StackLayout>
        <Label Text="Nazwa Twojego Stoiska" FontSize="24" HorizontalOptions="Center"  IsVisible="false" VerticalOptions="End" x:Name="NazwaStoiskaLabel"/>
        <Button Text="Wybierz stoisko" Clicked="OnWybierzStoiskoClicked" IsVisible="false" x:Name="WybierzStoiskoButton"/>
        <Button Text="Dodaj sprzedaż" Clicked="OnDodajSprzedazClicked" IsVisible="False" x:Name="DodajSprzedazButton"/>
        <Button Text="Zobacz sprzedaż" Clicked="OnZobaczSprzedazClicked" IsVisible="False" x:Name="ZobaczSprzedazButton"/>
        <Button Text="Wyloguj się" Clicked="OnLogoutButtonClicked" IsVisible="true" x:Name="LogoutButton"/>
       

    </StackLayout>
</ContentPage>
